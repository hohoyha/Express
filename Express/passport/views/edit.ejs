<% include ./partials/login %>

<% if (message) { %>
<form action="/api/message/update/<%= id %>"  method="post">
	<div>
	<label>Title:</label>
	<input type="text" name="title" value = '<%= message.title %>'>  </input> <br/>
	</div>
	<div>
    </br>
	<label>Content:</label>
	<textarea name="content" id="" cols="30" rows="10"><%= message.content %></textarea>
	</div>
	<div>
	<input type="submit" value="Submit"/>
	</div>
</form>

<%  }  %>